#!/usr/bin/env perl
use warnings;
use strict;
use FindBin;
use File::Spec;
# add current directory on the fly
use lib File::Spec->catfile($FindBin::Bin);
# import all
use branchLib;
use controller;
use pathLib;
use commitTreeLib;
use fileLib;
use helperLib;
use textLib;
use typeLib;
# this script for testing
my @src = get_content("mkSubmit");
my @dest = get_content("autoTest");


my %diff = diff(@src,@dest);

my @result =  patch(@src,%diff);

if (is_diff(@result, @dest)) {
  print "test Failed";
}
